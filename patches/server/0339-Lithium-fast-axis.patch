From: M0diis <modestas.kazlauskas@ktu.edu>
Date: Fri, 19 Jan 2024 22:32:42 +0200
Subject: [PATCH] Lithium fast axis


diff --git a/src/main/java/net/minecraft/world/phys/AABB.java b/src/main/java/net/minecraft/world/phys/AABB.java
index 5c70aa2ce1a9e89c8c271201b6755ea1325058ac..4f15f01c84133419f484242534b9cf6e84f82823 100644
--- a/src/main/java/net/minecraft/world/phys/AABB.java
+++ b/src/main/java/net/minecraft/world/phys/AABB.java
@@ -16,6 +16,13 @@ public class AABB {
     public final double maxY;
     public final double maxZ;
 
+    static {
+        assert Direction.Axis.X.ordinal() == 0;
+        assert Direction.Axis.Y.ordinal() == 1;
+        assert Direction.Axis.Z.ordinal() == 2;
+        assert Direction.Axis.values().length == 3;
+    }
+
     public AABB(double x1, double y1, double z1, double x2, double y2, double z2) {
         this.minX = Math.min(x1, x2);
         this.minY = Math.min(y1, y2);
@@ -81,11 +88,33 @@ public class AABB {
     }
 
     public double min(Direction.Axis axis) {
-        return axis.choose(this.minX, this.minY, this.minZ);
+        // SlimyPurpur start - fast axis
+        switch (axis.ordinal()) {
+            case 0: //X
+                return this.minX;
+            case 1: //Y
+                return this.minY;
+            case 2: //Z
+                return this.minZ;
+        }
+        // SlimyPurpur end
+
+        throw new IllegalStateException("Invalid axis " + axis);
     }
 
     public double max(Direction.Axis axis) {
-        return axis.choose(this.maxX, this.maxY, this.maxZ);
+        // SlimyPurpur start - fast axis
+        switch (axis.ordinal()) {
+            case 0: //X
+                return this.maxX;
+            case 1: //Y
+                return this.maxY;
+            case 2: //Z
+                return this.maxZ;
+        }
+        // SlimyPurpur end
+
+        throw new IllegalStateException("Invalid axis " + axis);
     }
 
     @Override
